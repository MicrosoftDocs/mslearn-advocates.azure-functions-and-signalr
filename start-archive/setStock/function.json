{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 * * * * *"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "stocksdb",
      "collectionName": "stocks",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOSDB_CONNECTION_STRING"
    }
  ]
}
