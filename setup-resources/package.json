{
    "name": "setup",
    "version": "1.0.0",
    "main": "upload.js",
    "scripts": {
        "start": "npm run setup:start && npm run upload && npm run setup:codespace-url",
        "prestart": "npm run clean && npm run build",
        "clean": "rimraf dist",
        "build": "tsc",
        "setup:codespace-url": "bash codespace-url.sh",
        "setup:start": "bash create-start-resources.sh",
        "setup:signalr": "bash create-signalr-resources.sh",
        "upload": "cp ../.env .env && node dist/upload.js"
    },
    "license": "ISC",
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.3"
    }
}
